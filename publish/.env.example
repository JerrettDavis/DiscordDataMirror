# Discord Data Mirror - Docker Compose Environment Variables
# Copy this file to .env and fill in the values

# PostgreSQL password (generate a secure password)
POSTGRES_PASSWORD=your-secure-password-here

# Discord bot token (from Discord Developer Portal)
DISCORD_TOKEN=your-discord-bot-token-here

# Dashboard HTTP port
DASHBOARD_PORT=8080

# Container images (built via dotnet publish or pulled from registry)
DASHBOARD_IMAGE=discorddatamirror-dashboard:latest
BOT_IMAGE=discorddatamirror-bot:latest
